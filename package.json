{
  "name": "cybersource-gate-way",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "build:babel": "babel src -d dist --copy-files",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "start": "babel-node ./src/index.js",
    "test": "npm run db:reset:test && cross-env NODE_ENV=test jest --passWithNoTests --detectOpenHandles --runInBand --forceExit",
    "coverage": "cross-env DB_ENV=test jest --detectOpenHandles --verbose --coverage --forceExit",
    "migrate": "sequelize db:migrate",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "migrate:undo:test": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "migrate:undo:dev": "sequelize db:migrate:undo:all",
    "seed": "npm run db:reset:dev && sequelize db:seed:all",
    "db:reset:dev": "npm run migrate:undo:dev && npm run migrate",
    "db:reset:test": "npm run migrate:undo:test && npm run migrate:test",
    "lint": "eslint './src/**/?(*).js?(x)' --fix",
    "testingthings": "nodemon -r esm ./testingthings.js",
    "generate:migration:file": "sequelize migration:generate --name testing-stuff"
  },
  "jest": {
    "collectCoverage": false
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "http-status": "^1.5.0",
    "morgan": "^1.10.0",
    "mssql": "^6.3.0",
    "sequelize": "^6.3.5",
    "tedious": "^9.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "esm": "^3.2.25",
    "rm": "^0.1.8",
    "sequelize-cli": "^6.2.0"
  }
}
